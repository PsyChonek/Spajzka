{
  "name": "spajzka",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --no-daemon",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generateClient": "cd ./apps/web && npm run generateClient"
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "eslint-config-custom": "*",
    "prettier": "latest",
    "semantic-release": "^21.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig": "*",
    "tsup": "^6.7.0",
    "turbo": "^1.8.8"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "npm@8.19.1"
}
