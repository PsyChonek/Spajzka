		"schemas": {
			"UserDto": {
				"properties": {
					"id": {
						"type": "string"
					},
					"email": {
						"type": "string"
					},
					"displayName": {
						"type": "string"
					}
				},
				"required": [
					"id",
					"email",
					"displayName"
				],
				"type": "object",
				"additionalProperties": false
			},
			"CreateUserDto": {
				"properties": {
					"email": {
						"type": "string"
					},
					"password": {
						"type": "string"
