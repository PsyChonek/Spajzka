					"displayName": {
						"type": "string"
					}
				},
				"required": [
					"email",
					"password",
					"displayName"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {
			"jwt": {
				"type": "apiKey",
				"name": "Authorization",
				"in": "header"
			}
		}
	},
	"info": {
		"title": "spajzka-api",
		"version": "1.0.0",
		"description": "API for managing shopping lists",
		"contact": {
			"name": "API Support",
			"email": "support@example.com"
		}
